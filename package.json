    "next-intl": "^4.7.0",
    "next-seo": "^7.0.1",
    "prisma": "^6.16.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "resend": "^6.9.1",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0"
